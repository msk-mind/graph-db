[
    # specify directory to the delta table
    import-delta: 'file:///<path_to_delta_table>',

    # specify cipher query
    cipher: 'match (a:User)
             merge (b:UserGroup {firstName: a.firstName})
             merge (a)-[:belongs_to]->(b)
               on create set
                 b.count = coalesce(b.count, 0)+1
             return a, b',

    cipher: 'MATCH (p:Person)<-[:LIKES]-(t:Technology)'
]